{
  "name": "lumora-standard",
  "displayName": "Lumora - Dynamic Theme",
  "description": "AI-powered dynamic theme that adjusts based on your mood and productivity",
  "version": "1.0.0",
  "publisher": "lumora",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lumora.toggleThemeAdjustment",
        "title": "Lumora: Toggle Theme Adjustment"
      },
      {
        "command": "lumora.showProductivity",
        "title": "Lumora: Show Productivity Stats"
      },
      {
        "command": "lumora.togglePomodoro",
        "title": "Lumora: Toggle Pomodoro Timer"
      },
      {
        "command": "lumora.resetPomodoro",
        "title": "Lumora: Reset Pomodoro Timer"
      },
      {
        "command": "lumora.toggleEyeStrain",
        "title": "Lumora: Toggle Eye Strain Prevention"
      },
      {
        "command": "lumora.toggleSoundEffects",
        "title": "Lumora: Toggle Sound Effects",
        "category": "Lumora"
      },
      {
        "command": "lumora.toggleFocusMode",
        "title": "Lumora: Toggle Focus Mode",
        "category": "Lumora"
      },
      {
        "command": "lumora.showTimeStats",
        "title": "Lumora: Show Time Statistics",
        "category": "Lumora"
      },
      {
        "command": "lumora.toggleBreakReminder",
        "title": "Lumora: Toggle Break Reminders",
        "category": "Lumora"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/color": "^4.2.0",
    "@types/node": "^16.x",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "eslint": "^8.x",
    "typescript": "^4.x"
  },
  "dependencies": {
    "color": "^4.2.3"
  }
}
